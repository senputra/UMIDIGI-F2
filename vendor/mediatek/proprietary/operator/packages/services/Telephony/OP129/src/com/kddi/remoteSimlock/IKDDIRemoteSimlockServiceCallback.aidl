package com.kddi.remoteSimlock;

import java.util.List;
import com.kddi.remoteSimlock.SimLockData;

interface IKDDIRemoteSimlockServiceCallback {

    /**
    * Remote Lock/Unlock operation response
    *
    * @param token
    * Identifier for the client to match the request.
    *
    * @param responseCode
    * SIMLOCK_RESP_SUCCESS = 0;
    * SIMLOCK_RESP_FAIL = 1;
    *
    * @param simlockResponse
    * Status Response message of the remote lock/unlock operation generated by Network Lock Module
    *
    * @return None
    */
    void setSimLockPolicyDataResponse(in int token, in int responseCode, in int soltId);

    /**
    * Device's get whilte list SIMLock data Response
    *
    * @param token
    * Identifier for the client to match the request.
    *
    * @param responseCode
    * SIMLOCK_RESP_SUCCESS = 0;
    * SIMLOCK_RESP_FAIL = 1;
    *
    * @param slotId
    * Represent the soltId of the response for the corresponding request.
    *
    * @param simlockData
    * SimLockData structure would be {String IMSI, String GID1, String GID2}
    *
    * @return None
    */
    void getAllowedSimLockDataResponse(in int token, in int responseCode, in int slotId, in List<SimLockData> simlockData);

    /**
    * Device's get whilte list SIMLock data Response
    *
    * @param token
    * Identifier for the client to match the request.
    *
    * @param responseCode
    * SIMLOCK_RESP_SUCCESS = 0;
    * SIMLOCK_RESP_FAIL = 1;
    *
    * @param slotId
    * Represent the soltId of the response for the corresponding request.
    *
    * @param simlockData
    * SimLockData structure would be {String IMSI, String GID1, String GID2}
    *
    * @return None
    */
    void getExcludedSimLockDataResponse(in int token, in int responseCode, in int slotId, in List<SimLockData> simlockData);

}
